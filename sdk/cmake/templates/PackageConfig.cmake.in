@PACKAGE_INIT@

# LinxSDK Package Configuration File
# This file is used by find_package() to locate the LinxSDK package

# Set the version
set(@PROJECT_NAME@_VERSION "@PROJECT_VERSION@")

# Include the targets file
include("${CMAKE_CURRENT_LIST_DIR}/@PROJECT_NAME@Targets.cmake")

# Check required components
check_required_components(@PROJECT_NAME@)

# Set variables for compatibility
set(@PROJECT_NAME@_FOUND TRUE)
set(@PROJECT_NAME@_LIBRARIES @PROJECT_NAME@::@PROJECT_NAME@)
set(@PROJECT_NAME@_INCLUDE_DIRS "${PACKAGE_PREFIX_DIR}/include")

# Provide information about the package
message(STATUS "Found @PROJECT_NAME@: ${@PROJECT_NAME@_VERSION}")